{
  "name": "image-galleries",
  "version": "2.0.21",
  "displayName": "Image Galleries",
  "author": "bakatrouble",
  "description": "Image galleries on some websites",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://example.com\" --source-dir ./dist/",
    "sign": "pnpm build && dotenv cross-var web-ext sign",
    "lint": "eslint --cache . --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/eslint-parser": "^7.21.8",
    "@samrum/vite-plugin-web-extension": "^4.0.0",
    "@types/await-timeout": "^0.3.1",
    "@types/chrome": "^0.0.228",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^9.0.1",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "eslint-config-alloy": "^5.0.0",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.6.5",
    "web-ext": "^7.6.2"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@mdi/js": "^7.2.96",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vueuse/gesture": "^2.0.0-beta.1",
    "@vueuse/head": "^1.1.26",
    "await-timeout": "^1.1.1",
    "axios": "^1.6.0",
    "csscalc": "^0.1.2",
    "lethargy-ts": "^0.0.5",
    "lodash": "^4.17.21",
    "portal-vue": "^3.0.0",
    "qs": "^6.11.2",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vuetify": "^3.3.2",
    "webextension-polyfill": "^0.10.0"
  },
  "packageManager": "pnpm@8.5.1",
  "webExt": {
    "sourceDir": "dist/"
  }
}
